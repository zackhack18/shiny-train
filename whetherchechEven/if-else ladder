/*Write a program to print grade of a student using if else ladder statement*/
#include <stdio.h>
int main()
{
    int marks;
    printf("Enter the your marks between 1-100\n");
    scanf("%d", &marks);
    
    if(marks>=90)
    printf("Grade:A\n");

    else if(marks>=80)
    printf("Grade:B\n");

    else if(marks>=70)
    printf("Grade:C\n");

    else if(marks>=50 && marks<70)
    printf("Grade:D\n");

    else
    printf(Grade:failed\n);
    return 0;
}
