/*Write a program to check whether an input no. is prime or not*/
#include <stdio.h>
void prime (int);
void main()
{
    int n;
    printf("Enter a number");
    scanf("%d", &n);
    prime(n);
}
void prime(int a)
{
    int prime = 1, i=2;
    while(i<=n?2)
    {
        if(n%i==0)
        {
            prime = 0;
            break;
        }
        i++;
    }
    if(prime==1)
    printf("prime");
    else
    printf("not a prime");
}
