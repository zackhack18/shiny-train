/*Write a program to find minimum & maximum elements of an array*/
#include <stdio.h>
void main()
{
    int A[10], i, min, max;
    printf("Enter the array elements\n");
    for(i=0; i<9; i++)
    scanf("%d", &A[i]);
    min = max = A[0];

    for(i=0; i<9; i++)
    {
        if(A[i]<min)
        min = A[i];

        if(A[i]>max)
        max = A[i];
    }
    printf("minimum = %d\n maximum = %d", min,max);
}
