/*Write a program to calculate xY using recursion*/
#include <stdio.h>
int power(int,int)
void main()
{
    int x, y, m;
    printf("Enter x and y");
    scanf("%d %d", &x, &y);
    m = power(x,y);
    printf("result = %d", m);
}
int power(int x, int y)
{
    if(y==0)
    return 1;
    else
    return x* power(x,y-1);
}
