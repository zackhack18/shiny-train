/*Write a program to check whether a given year  is leap or not*/
#include <stdio.h>
int main()
{
    int year;
    printf("Enter a year to check\n");
    scanf("%d", &year);

    if((year4==0)&&(year%100!=0)||(year%400==0))
    printf("%d is a leap year\n", &year);

    else
    printf("%d is not a leap year\n", year);
    return 0;
}
