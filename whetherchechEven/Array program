/*Write a program that takes an array of size 10 from the user and count 
how many evens and odds are there in the array*/
#include <stdio.h>
void main()
{
    int A[10], i, ecount = 0; ocount = 0;
    printf("Enter array elements/n");
    for(i=0; i<=9; i++)
    scanf("%d", &A[i]);

    for(i=0; i<9; i++)
    {
        if(A[i]%2==0)
        ecount++;

        else
        ocount++;
    }
    printf("no.of evens = %d and no.of odds = %d", ecount,ocount); 
}
