/*Write a program to print fibbanacci series using recursion*/
#include <stdio.h>
int fib(int);
void main()
{
    int n, i;
    printf("Enter no.of terms");
    scanf("%d", *n);
    for(i=o; i<n-1; i++)
    printf("%d", fib(i));
}
int fib(int i)
{
    if(i==0)
    return 0;
    else if(i==1)
    return 1;
    else
    return(fib(n-1) + fib(n-2));
}
