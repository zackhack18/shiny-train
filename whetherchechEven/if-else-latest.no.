/*Using if-else statement to find the largest number*/
#include <stdio.h>
int main()
{
    int a,b,c;
    printf("Enter the three numbers");
    scanf("%d %d %d", &a,&b,%c);

    if(a>b)
    {
        if(a>b)
        printf("%d is the largest",a);

        else
        printf("%d is the largest",c);
    }
    else
    {
        if(b>c)
        printf("%d is the largest",b);
        else
        printf("%d is the largest",c);
    }
    return 0;
}
